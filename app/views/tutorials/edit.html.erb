<h1>Edit Tutorial</h1>
<%= form_tag update_tutorial_path do %>
  <p>
    <%= label_tag :id, "ID"  %>     
    <%= text_field_tag :id, @tutorial.id %></br> 
    <%= label_tag :user, "User"  %>     
    <%= text_field_tag :user, @tutorial.user %></br>
    <%= label_tag :title, "Title"  %>     
    <%= text_field_tag :title, @tutorial.title %></br>
    <%= label_tag :description, "Description"  %>     
    <%= text_field_tag :description, @tutorial.description %></br>
    <%= submit_tag "Update Tutorial" %> 
  </p>
<% end %>